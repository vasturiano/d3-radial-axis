{
  "name": "d3-radial-axis",
  "version": "1.7.2",
  "description": "Radial axes for D3 components.",
  "license": "MIT",
  "keywords": [
    "d3",
    "d3-module",
    "axis",
    "radial",
    "polar",
    "spiral",
    "scale",
    "visualization"
  ],
  "type": "module",
  "unpkg": "dist/d3-radial-axis.min.js",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": {
    "umd": "./dist/d3-radial-axis.js",
    "default": "./src/index.js"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/d3-radial-axis.git"
  },
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://github.com/vasturiano"
  },
  "homepage": "https://github.com/vasturiano/d3-radial-axis#readme",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "prepare": "npm run build"
  },
  "dependencies": {
    "d3-scale": "1 - 4",
    "d3-shape": "1 - 3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "rimraf": "^4.1.2",
    "rollup": "^3.14.0"
  },
  "engines": {
    "node": ">=12"
  }
}
